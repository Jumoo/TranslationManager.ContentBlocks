  <area alias="translateHealthCheck">
    <key alias="orphanCheck">Clear orphaned nodes</key>
    <key alias="orphanTitle">Orphaned nodes</key>
    <key alias="orphanFail">There are %0% orphaned nodes with no associated job</key>
    <key alias="orphanPass">There are no orphaned nodes</key>
    <key alias="orphanClean">Orphaned nodes removed</key>

    <key alias="workingCheck">Clear Working Set nodes</key>
    <key alias="workingTitle">Working Set Nodes</key>
    <key alias="workingFail">
      <![CDATA[
      <p>
        There are %0% nodes in the working set status
      </p>
      <p>
        Working set nodes are created as items are being sent to translation. if the 
        send process fails items can be left in this state
      </p>
      <p>
        there is a scheduled task to clear old working set items so you shouldn't need
        to clear them here
      </p>
      <p><em>check no-one is currently creating a translation job before clearing these values</em></p>
      ]]>
    </key>
    <key alias="workingPass">There are no working set nodes</key>
    <key alias="workingClean">Working items cleaned</key>

    <key alias="propertyCheck">Clear orphaned properties</key>
